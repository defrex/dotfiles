[user]
	name = Aron Jones
	email = aron.jones@gmail.com

[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[color]
	ui = true
[push]
	default = upstream
[core]
	autocrlf = input
	safecrlf = true
[credential]
	helper = cache
